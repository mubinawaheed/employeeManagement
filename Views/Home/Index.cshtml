@model IEnumerable<employeeManagement.Models.Employee>

<h3>Employee List</h3>
	<div>
	<table>
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Department</th>
				<th>Email</th>
			</tr>
		</thead>
		<tbody>
		@foreach(var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.Department</td>
				<td>@employee.Email</td>
			</tr>
				
		}
	</tbody>
	</table>
</div>
